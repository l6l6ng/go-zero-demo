syntax = "v1"

info(
    title: "type title here"
    desc: "type desc here"
    author: "type author here"
    email: "type email here"
    version: "type version here"
)

type (
    Req {
        Name string `json:"name"`
    }

    Rep {
        name string  `json:"name"`
    }
)

@server(
    jwt: Auth
    group: template
)
service template {
    @handler handlerName
    get /users/id/:userId (Rep) returns (Rep)
}
